<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bitsi34gb.emercencylifesaver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/emeric"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/emeric"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
        <activity android:name=".ListOfDonors"></activity>
        <activity android:name=".LastDonated" />
        <activity android:name=".ProfilePage" />
        <activity
            android:name=".UsersActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Blooddonar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>